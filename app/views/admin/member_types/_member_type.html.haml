%tr{:member_type => member_type.id}
  %td.name= link_to member_type.name, admin_member_type_path(member_type)
  %td.description= member_type.description
  %td.price= number_to_currency(member_type.price, :precision => 2)
  %td.options
    = edit_model_link member_type
    = destroy_model_link member_type

